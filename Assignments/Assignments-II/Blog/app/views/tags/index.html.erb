<p id="notice"><%= notice %></p>

<h1>Tags</h1>
<%= link_to 'New Tag', new_tag_path, class: "btn btn-default"  %>
<table>
  <thead>
    <tr>
      <th>Name</th>
    </tr>
  </thead>
  <tbody>
    <% @tags.each do |tag| %>
      <tr>
        <td><%= link_to tag.name, edit_tag_path(tag) %></td>  
      </tr>
    <% end %>
  </tbody>
</table>

<br>


