<p id="notice"><%= notice %></p>

<h1>Categories</h1>
<%= link_to 'New Categories', new_category_path, class: "btn btn-default"  %>
<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @categories.each do |category| %>
      <tr>
        <td><%= link_to category.name, category %></td>
        
      </tr>
    <% end %>
  </tbody>
</table>

