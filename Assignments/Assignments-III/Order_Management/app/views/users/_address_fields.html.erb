<fieldset>


  <div class="field">
    <%= f.label :line1 %>
    <%= f.text_field :line1, id: :address_line1 %>
  </div>

  <div class="field">
    <%= f.label :line2 %>
    <%= f.text_field :line2, id: :address_line2 %>
  </div>

  <div class="field">
    <%= f.label :line3 %>
    <%= f.text_field :line3, id: :address_line3 %>
  </div>

  <div class="field">
    <%= f.label :city %>
    <%= f.text_field :city, id: :address_city %>
  </div>

  <div class="field">
    <%= f.label :state %>
    <%= f.text_field :state, id: :address_state %>
  </div>

  <div class="field">
    <%= f.label :country %>
    <%= f.text_field :country, id: :address_country %>
  </div>

  <div class="field">
    <%= f.label :postal_code %>
    <%= f.text_field :postal_code, id: :address_postal_code %>
  </div>

  <div class="field">
    <%= f.label :contact_no %>
    <%= f.text_field :contact_no, id: :address_contact_no %>
  </div>

  <div class="field">
    <%= f.label :email %>
    <%= f.email_field :email, id: :address_email %>
  </div>

  <div class="field">
    <%= f.hidden_field :_destroy %>
    <%= link_to "remove", '#', class: "remove_fields" %>
  </div>
  <hr>
</fieldset>
